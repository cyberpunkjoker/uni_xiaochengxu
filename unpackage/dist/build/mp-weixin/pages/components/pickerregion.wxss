.input-flex-select{width:100%}.pickerBox{position:fixed;left:0;right:0;bottom:0;z-index:99;padding:10px 20px;background-color:#fff;text-align:center}.flexbox{display:-webkit-box;display:-webkit-flex;display:flex;margin-bottom:20px;padding-bottom:24px;border-bottom:1px solid #f2f2f2}.cancel{-webkit-box-flex:1;-webkit-flex:1;flex:1;font-size:16px;font-weight:500;color:#939699}.content{-webkit-box-flex:2;-webkit-flex:2;flex:2;font-size:16px;font-weight:500;color:#4b4c4d}.sure{-webkit-box-flex:1;-webkit-flex:1;flex:1;font-size:16px;font-weight:500;color:#ffcb29}